<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="users">
	
	<insert id="userJoin" parameterType="com.javaex.vo.UserVo">
	  	<![CDATA[
	  		insert into users
			values(SEQ_USERS_NO.nextval,{id},{pw},{hp},{nickName},{email},'user')
	  	]]>
	</insert>

	<insert id="hostJoin" parameterType="com.javaex.vo.UserVo">
	  	<![CDATA[
	  		insert into users
			values(SEQ_USERS_NO.nextval,{id},{pw},{hp},{nickName},{email},'host')
	  	]]>
	</insert>
	
</mapper>