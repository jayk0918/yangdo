<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace = "Hostcompanyreg">
	<select id = "email" resultType = "com.javaex.vo.HostCompanyregVo">
	
	<![CDATA[
		select  email
		from users
		where no = 3
		and id = #{id}
	]]>
	
	</select>
	
	<insert id = "insertcompany" parameterType = "com.javaex.vo.HostCompanyregVo">
	
	<![CDATA[
		insert into company
		values(SEQ_COMPANY_NO.nextval, 3, #{companyHp}, #{ceoHp}, #{bankName}, #{accountHolder}, #{account}, #{settlementCycle}, #{taxInvoice}, #{companyName}, #{ceoName}, #{businessLicensenumber}, #{postalCode}, #{companyAddress}, #{detailAddress}, #{homepage}, #{taxInvoiceEmail})
	]]>
	
	</insert>
	
	<insert id = "insertBTA" parameterType = "int">

		<![CDATA[
		insert into c_b_type
		values (seq_cbt_no.nextval, 1, #{insertBTArrays})
		]]>
	
	</insert>
	
	<insert id = "insertBGA" parameterType = "int">
	
		<![CDATA[
		insert into c_b_group
		values (seq_cbg_no.nextval, 1, #{insertBGArrays})
		]]>
		
	</insert>
	
</mapper>